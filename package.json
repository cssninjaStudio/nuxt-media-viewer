{
  "name": "@cssninja/nuxt-media-viewer",
  "version": "0.0.4",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "pnpm build",
    "dev": "nuxi dev src/client",
    "dev:playground": "pnpm build && nuxi dev playground",
    "dev:prepare": "nuxi prepare src/runtime",
    "build": "pnpm dev:prepare && pnpm build:module && pnpm build:client",
    "build:client": "nuxi generate src/client",
    "build:module": "nuxt-module-build",
    "release": "run-s release:*",
    "release:standard-version": "standard-version",
    "release:publish": "git push --follow-tags origin main && npm publish"
  },
  "dependencies": {
    "@nuxt/devtools-ui-kit": "^0.2.3",
    "@vueuse/core": "^9.2.0",
    "@vueuse/nuxt": "^9.2.0",
    "chalk": "^5.0.1",
    "consola": "^2.15.3",
    "date-fns": "^2.29.3",
    "etag": "^1.8.1",
    "image-size": "^1.0.2",
    "ipx": "1.0.0-2",
    "pathe": "^1.1.0",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "0.2.3",
    "@nuxt/module-builder": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/node": "^18.14.0",
    "eslint": "latest",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.2.0",
    "sirv": "^2.0.2",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.5"
  }
}
